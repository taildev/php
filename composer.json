{
    "name": "taildev/php",
    "description": "PHP integration for tail.dev",
    "type": "library",
    "keywords": ["tail.dev", "monitoring", "apm", "exceptions", "logging"],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "6.*|7.*",
        "monolog/monolog": "1.*|2.*",
        "nesbot/carbon": "2.*",
        "psr/log": "1.*",
        "ramsey/uuid": "^3.9|^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Tail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "support": {
        "issues": "https://github.com/taildev/php/issues",
        "source": "https://github.com/taildev/php"
    },
    "scripts": {
        "fmt": "./vendor/bin/phpcbf",
        "lint": "./vendor/bin/phpcs",
        "test": "./vendor/bin/phpunit"
    },
    "archive": {
        "exclude": [
            "tests"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
